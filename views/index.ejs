                                        <div class="card-title">
                                            <h2>
                                                <%=element.postheading%>
                                            </h2>
                                        </div>
                                        <p class="card-text p-tb  link-bottom">
                                            <%= element.postname %>
                                        </p>
                                        <div class="flex justify-center">
                                            <a href="/<%= element.postheading %>" class="link">Go somewhere</a>
