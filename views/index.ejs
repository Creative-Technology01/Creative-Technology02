                    <% latestposts.reverse().forEach(element=> { %>
                        <div class="flex space-evenly space">
                            <div class="3pair-set">
                                <div class="card">
                                    <img src="/images/upload/<%= element.image %>"
                                        class="card-img-top p-tb flex justify-center" alt="...">

